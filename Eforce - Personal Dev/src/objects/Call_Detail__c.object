    <validationRules>
        <fullName>Product_Feedback_Mandatory</fullName>
        <active>true</active>
        <description>Product feedback is mandatory field during Call execution</description>
        <errorConditionFormula>ISPICKVAL(Call__r.Status__c ,&quot;Completed&quot;) &amp;&amp; (isblank( Product_Feedback__c ))</errorConditionFormula>
        <errorMessage>Please enter Product Feedback comments during submission of a call.</errorMessage>
    </validationRules>
</CustomObject>
