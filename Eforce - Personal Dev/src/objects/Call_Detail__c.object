        <label>Detailed</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Is_Planned_Product__c</fullName>
        <defaultValue>false</defaultValue>
        <label>Is Planned Product</label>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Product_Feedback__c</fullName>
        <label>Product Feedback</label>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Product_Group__c</fullName>
        <description>TO fetch the product group of a specific brand</description>
        <formula>Product__r.Parent_Product__r.Name</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Product Group</label>
        <type>Text</type>
    </fields>
    <fields>
        <fullName>Product__c</fullName>
        <label>Product</label>
        <referenceTo>MyProduct__c</referenceTo>
